#include<iostream>
using namespace std;
int main()
{
    int arr[]={2,9,1,2,5,2,8,2,23,11,9,4};
    for(int i=0;i<12;i++)
    cout<<arr[i]<<" ";
    cout<<endl;
    for(int i=0;i<12;i++)
    for(int j=i;j<12;j++)
    if(arr[i]>arr[j])
    {  
    int temp=arr[i];
    arr[i]=arr[j];
    arr[j]=temp;
    }
    for(int i=0;i<12;i++)
    cout<<arr[i]<<" ";
}
